{
  "schemaVersion": 1,
  "id": "jmc",
  "version": "${version}",
  "name": "Just More Cakes!",
  "description": "Adds 30 new flavors of cake along with new improvements and mechanics for a better cake experience",
  "authors": [
    "MincraftEinstein"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/just-more-cakes",
    "sources": "https://github.com/MincraftEinstein/JustMoreCakes",
    "issues": "https://github.com/MincraftEinstein/JustMoreCakes/issues"
  },
  "license": "All rights reserved",
  "icon": "pack.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "einstein.jmc.JustMoreCakesFabric"
    ],
    "client": [
      "einstein.jmc.JustMoreCakesFabric"
    ],
    "fabric-datagen": [
      "einstein.jmc.JustMoreCakesFabric"
    ],
    "jei_mod_plugin": [
      "einstein.jmc.compat.jei.ModJEIPlugin"
    ],
    "rei_client": [
      "einstein.jmc.compat.rei.ModREIClientPlugin"
    ],
    "rei_common": [
      "einstein.jmc.compat.rei.ModREICommonPlugin"
    ],
    "jade": [
      "einstein.jmc.compat.jade.ModJadePlugin"
    ]
  },
  "mixins": [
    "jmc.fabric.mixins.json",
    "jmc.mixins.json"
  ],
  "accessWidener": "jmc.accesswidener",
  "depends": {
    "fabricloader": ">=${fabric_version}",
    "fabric-api": ">=${fabric_api_version}",
    "minecraft": ">=${minecraft_version} <${max_minecraft_version}",
    "java": ">=${java_version}",
    "forgeconfigapiport": ">=8.0.0",
    "commonnetworking": ">=1.0.3-1.20.1"
  },
  "suggests": {
    "jei": ">=15.3.0.4",
    "configured": ">=2.0.2",
    "cakechomps": ">0",
    "roughlyenoughitems": ">=12.0.684",
    "jade": ">=11.8.0",
    "amendments": ">=1.20-1.1.6"
  },
  "custom": {
    "catalogue": {
      "banner": "pack.png"
    },
    "modmenu": {
      "links": {
        "modmenu.modrinth": "https://modrinth.com/mod/just-more-cakes",
        "modmenu.curseforge": "https://curseforge.com/minecraft/mc-mods/just-more-cakes",
        "modmenu.discord": "https://discord.gg/gSsaFAvrBM",
        "modmenu.kofi": "https://ko-fi.com/mincrafteinstein"
      }
    }
  }
}
